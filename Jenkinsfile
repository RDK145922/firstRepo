pipeline {
  agent any
  stages {
    stage ("build") {
      steps {
        sh 'python --version'
        echo "build..."
        
      }
    }
    tage ("test") {
      steps {
        sh 'python --version'
        echo "test..."
      }
    }
    
    tage ("deploy") {
      steps {
        sh 'python --version'
        echo "deploy..."
      }
    }
    
  }
  
}  
